<?xml version="1.0" encoding="utf-8" ?>
<root>
  <name>疫苗</name>
  <name_zh-cn>疫苗</name_zh-cn>
  <name_zh-hk>疫苗</name_zh-hk>
  <name_en-us>Vaccine</name_en-us>
  <tablename>dr_tb_vaccine</tablename>
  <searchcondition>r_main.status&lt;&gt;'D'</searchcondition>
  <ismutillang>1</ismutillang>
  <fields>
    <field>
      <name>疫苗编号</name>
      <name_zh-cn>疫苗编号</name_zh-cn>
      <name_zh-hk>疫苗编号</name_zh-hk>
      <name_en-us>Vaccine Number</name_en-us>
      <key>code</key>
      <type>text</type>
      <displayinlist>1</displayinlist>
      <gotodetail>1</gotodetail>
      <notnull>1</notnull>
      <search>1</search>
      <disableindetail>1</disableindetail>
    </field>
    <field>
      <name>疫苗名称</name>
      <name_zh-cn>疫苗名称</name_zh-cn>
      <name_zh-hk>疫苗名称</name_zh-hk>
      <name_en-us>Vaccine Name</name_en-us>
      <key>name</key>
      <type>text</type>
      <ismutillang>1</ismutillang>
      <displayinlist>1</displayinlist>
      <gotodetail>1</gotodetail>
      <search>1</search>
      <notnull>1</notnull>
    </field>
    <field>
      <name>首页显示</name>
      <name_zh-cn>首页显示</name_zh-cn>
      <name_zh-hk>首页显示</name_zh-hk>
      <name_en-us>Display In Index</name_en-us>
      <key>is_index</key>
      <type>check</type>
      <displayinlist>1</displayinlist>
      <yvalue>是</yvalue>
      <nvalue>否</nvalue>
    </field>
    <field>
      <name>显示图片</name>
      <name_zh-cn>显示图片</name_zh-cn>
      <name_zh-hk>显示图片</name_zh-hk>
      <name_en-us>Image</name_en-us>
      <key>image</key>
      <type>upload</type>
      <uploadmodule>vaccine</uploadmodule>
    </field>
    <field>
      <name>治疗效果</name>
      <name_zh-cn>治疗效果</name_zh-cn>
      <name_zh-hk>治疗效果</name_zh-hk>
      <name_en-us>Effect</name_en-us>
      <key>effect_id</key>
      <type>fkey</type>
      <displayfield>name</displayfield>
      <displayinlist>1</displayinlist>
      <gotodetail>1</gotodetail>
      <search>1</search>
      <notnull>1</notnull>
      <fmutillang>1</fmutillang>
      <tablename>dr_tb_effect</tablename>
      <ntbname>r_effect</ntbname>
      <condition>r_effect.status='A' and ( r_effect.type='A' or r_effect.type='Y' )</condition>
    </field>
    <field>
      <name>接种次数</name>
      <name_zh-cn>接种次数</name_zh-cn>
      <name_zh-hk>接种次数</name_zh-hk>
      <name_en-us>Injection Count</name_en-us>
      <key>injection_count</key>
      <type>number</type>
      <notnull>1</notnull>
    </field>
    <field>
      <name>接种时间</name>
      <name_zh-cn>接种时间</name_zh-cn>
      <name_zh-hk>接种时间</name_zh-hk>
      <name_en-us>Injection Time</name_en-us>
      <key>injection_time</key>
      <type>longtext</type>
      <ismutillang>1</ismutillang>
      <notnull>1</notnull>
    </field>
    <field>
      <name>接种季节</name>
      <name_zh-cn>接种季节</name_zh-cn>
      <name_zh-hk>接种季节</name_zh-hk>
      <name_en-us>Injection Season</name_en-us>
      <key>injection_season</key>
      <type>longtext</type>
      <ismutillang>1</ismutillang>
      <notnull>1</notnull>
    </field>
    <field>
      <name>适用人群</name>
      <name_zh-cn>适用人群</name_zh-cn>
      <name_zh-hk>适用人群</name_zh-hk>
      <name_en-us>User Group</name_en-us>
      <key>used_group_id</key>
      <type>flist</type>
      <displayfield>name</displayfield>
      <displayinlist>1</displayinlist>
      <fmutillang>1</fmutillang>
      <notnull>1</notnull>
      <tablename>dr_tb_used_group</tablename>
      <ntbname>r_used_group</ntbname>
      <condition>r_used_group.status='A' and ( r_used_group.type='A' or r_used_group.type='Y' )</condition>
    </field>
    <field>
      <name>香港计划内</name>
      <name_zh-cn>香港计划内</name_zh-cn>
      <name_zh-hk>香港计划内</name_zh-hk>
      <name_en-us>Is Hongkong Plan</name_en-us>
      <key>is_hk_project</key>
      <type>check</type>
      <displayinlist>1</displayinlist>
      <yvalue>是</yvalue>
      <nvalue>否</nvalue>
    </field>
    <field>
      <name>中国计划内</name>
      <name_zh-cn>中国计划内</name_zh-cn>
      <name_zh-hk>中国计划内</name_zh-hk>
      <name_en-us>Is Chinese Plan</name_en-us>
      <key>is_china_project</key>
      <type>check</type>
      <displayinlist>1</displayinlist>
      <yvalue>是</yvalue>
      <nvalue>否</nvalue>
    </field>
    <field>
      <name>疫苗详细介绍</name>
      <name_zh-cn>疫苗详细介绍</name_zh-cn>
      <name_zh-hk>疫苗详细介绍</name_zh-hk>
      <name_en-us>Vaccine Desciption</name_en-us>
      <key>description</key>
      <type>longtext</type>
      <ismutillang>1</ismutillang>
      <notnull>1</notnull>
    </field>
    <field>
      <name>接种注意事项</name>
      <name_zh-cn>接种注意事项</name_zh-cn>
      <name_zh-hk>接种注意事项</name_zh-hk>
      <name_en-us>Injection Caution</name_en-us>
      <key>caution</key>
      <type>longtext</type>
      <ismutillang>1</ismutillang>
      <notnull>1</notnull>
    </field>
    <field>
      <name>可能产生的不良反应</name>
      <name_zh-cn>可能产生的不良反应</name_zh-cn>
      <name_zh-hk>可能产生的不良反应</name_zh-hk>
      <name_en-us>Bad Effect</name_en-us>
      <key>bad_effect</key>
      <type>longtext</type>
      <ismutillang>1</ismutillang>
      <notnull>1</notnull>
    </field>
    <field>
      <name>产生不良反应的解决方法</name>
      <name_zh-cn>产生不良反应的解决方法</name_zh-cn>
      <name_zh-hk>产生不良反应的解决方法</name_zh-hk>
      <name_en-us>Solutions</name_en-us>
      <key>bad_effect_resolve</key>
      <type>longtext</type>
      <ismutillang>1</ismutillang>
      <notnull>1</notnull>
    </field>
    <field>
      <name>疫苗接种标准价格</name>
      <name_zh-cn>疫苗接种标准价格</name_zh-cn>
      <name_zh-hk>疫苗接种标准价格</name_zh-hk>
      <name_en-us>Standard Price</name_en-us>
      <key>standard_price</key>
      <type>number</type>
      <notnull>1</notnull>
    </field>
    <field>
      <name>折扣</name>
      <name_zh-cn>折扣</name_zh-cn>
      <name_zh-hk>折扣</name_zh-hk>
      <name_en-us>Discount</name_en-us>
      <key>discount</key>
      <type>number</type>
      <notnull>1</notnull>
    </field>
    <field>
      <name>页面详细介绍</name>
      <name_zh-cn>页面详细介绍</name_zh-cn>
      <name_zh-hk>页面详细介绍</name_zh-hk>
      <name_en-us>Html Description</name_en-us>
      <key>context</key>
      <type>html</type>
    </field>
    <field>
      <name>备注</name>
      <name_zh-cn>备注</name_zh-cn>
      <name_zh-hk>备注</name_zh-hk>
      <name_en-us>Remarks</name_en-us>
      <key>remarks</key>
      <type>longtext</type>
    </field>
    <field>
      <name>状态</name>
      <name_zh-cn>状态</name_zh-cn>
      <name_zh-hk>状态</name_zh-hk>
      <name_en-us>Status</name_en-us>
      <key>status</key>
      <type>select</type>
      <displayinlist>1</displayinlist>
      <options>
        <option>
          <name>启用</name>
          <name_zh-cn>启用</name_zh-cn>
          <name_zh-hk>启用</name_zh-hk>
          <name_en-us>Active</name_en-us>
          <value>A</value>
        </option>
        <option>
          <name>禁用</name>
          <name_zh-cn>禁用</name_zh-cn>
          <name_zh-hk>禁用</name_zh-hk>
          <name_en-us>Inactive</name_en-us>
          <value>I</value>
        </option>
      </options>
      <search>1</search>
      <notnull>1</notnull>
    </field>
  </fields>
